#include <iostream>
using namespace std;

int main()
{
    int marks;
    cout<<"Enter the marks"<<endl;
    cin>>marks;
    
    if(marks>=90) {
        cout<<"A"<<" Outstanding"<<endl;
    }
    
    else if ((marks>=80)&& (marks<90)) {
        cout<<"B"<<" Verygood"<<endl;
    }
    else if ((marks>=70)&&(marks<80)) {
        cout<<"C"<<" Good"<<endl;
    }
    else {
        cout<<"D"<<" improve"<<endl;
    }
    return 0;
}
