//Sum of odd number from 1 to n.
#include <iostream>
using namespace std;

int main()
{
    int n;
    int sum=0;
    int i;
    cout<<"Enter the value of n"<<endl;
    cin>>n;
    
    for( i=1;i<=n;i++) {
        
     if(i%2!=0){
        
        sum+=i;
        
     }
    
    }
  
   cout<<sum<<endl;
    
    return 0;
}
