//print square pattern of continue counting of numbers
#include <iostream>
using namespace std;

int main()
{
    int n;
    int num=1;
    cout<<"enter the value of n"<<endl;
    cin>>n;
  
    
    for(int i=1;i<=n;i++) {
        
       for (int j=1;j<=n;j++){
           
           cout<<num<<" ";
           num++;
       }
       cout<<endl;
    }
    

    return 0;
}
