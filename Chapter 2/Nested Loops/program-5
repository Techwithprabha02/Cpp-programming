//print square pattern of continue alphabets
#include <iostream>
using namespace std;

int main()
{
    int n;
    char ch='A';
    cout<<"enter the value of n"<<endl;
    cin>>n;
  
    
    for(int i=1;i<=n;i++) {
        
       for (int j=1;j<=n;j++){
           
           cout<<ch<<" ";
           ch+=1;
       }
       cout<<endl;
    }
    

    return 0;
}
