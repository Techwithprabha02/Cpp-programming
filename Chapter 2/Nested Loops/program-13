//Print inverted triangle pattern for characters.
#include <iostream>
using namespace std;
int main() {
int n;




cout<<"Enter the value of n"<<endl;
cin>>n;

 
for(int i=0;i<n;i++) {
     char ch='A';
    
    for(int j=0;j<i;j++) {
        cout<<" ";
    }
    for(int k=0;k<(n-i);k++) {
        cout<<ch;
        ch+=1;
   
    }
    
    cout<<endl;
      
    
}
return 0;
}
