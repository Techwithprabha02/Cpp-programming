#include<iostream>
using namespace std;

int main() {
    int c,d;
    cout<<"Enter the value of c"<<endl;
    cin>>c;
    cout<<"enter the value of d"<<endl;
    cin>>d;
    
    cout<<endl;
    
    cout<< (c > d) <<endl;
    cout<< (c < d) <<endl;
    cout<< (c <= d) <<endl;
    cout<< (c >= d) <<endl;
    cout<< (c != d) <<endl;
    cout<<(c == d) <<endl;
    
    return 0;
}
